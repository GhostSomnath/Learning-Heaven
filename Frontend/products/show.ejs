<% layout('layouts/boilerplate') -%>

<section class="container mx-auto px-4 py-8 shopingmarg mb-56">
    <%- include('../partials/flash') %>

    <div class="flex flex-col lg:flex-row gap-6">
        <div class="w-full lg:w-1/2">
            <div class="bg-white shadow-lg rounded-lg overflow-hidden">
                <img src="<%= product.img %>" class="w-full h-64 object-cover" alt="item image">
                <div class="p-4">
                    <h5 class="text-xl font-semibold mb-2">
                        <%= product.name %>
                    </h5>
                    <p class="text-gray-700 mb-2">
                        <%= product.desc %>
                    </p>
                    <h5 class="text-xl font-bold mb-4">Rs. <%= product.price %></h5>
                    <button onclick="showPopup()"
                        class="inline-block bg-green-500 text-white py-2 px-4 rounded-lg hover:bg-green-600">Buy</button>
                    <form class="inline-block ml-2" action="/user/<%= product._id %>/add" method="POST">
                        <button type="submit"
                            class="bg-blue-500 text-white py-2 px-4 rounded-lg hover:bg-blue-600">Add To Cart</button>
                    </form>
                    <span class="cursor-pointer text-red-500 wishlist-toggle inline-block ml-2" data-product-id="<%= product._id %>">
                        <% if(currentUser && currentUser.wishList.includes(product._id)) { %>
                            <i class="fas fa-heart"></i>
                        <% } else { %>
                            <i class="far fa-heart"></i>
                        <% } %>
                    </span>
                    <% if (currentUser && currentUser.role === 'seller') { %>
                        <a href="/products/<%= product._id %>/edit"
                            class="inline-block bg-blue-500 text-white py-2 px-4 rounded-lg hover:bg-blue-600 mt-2">Edit</a>
                        <form class="inline-block ml-2" action="/products/<%= product._id %>?_method=DELETE" method="POST">
                            <button type="submit"
                                class="bg-red-500 text-white py-2 px-4 rounded-lg hover:bg-red-600">Delete</button>
                        </form>
                    <% } %>
                </div>
            </div>
        </div>

        <div class="w-full lg:w-1/3">
            <h1 class="text-2xl font-semibold mb-4">Leave Your Review</h1>
            <form action="/products/<%= product._id %>/review" method="POST">
                <div class="mb-4">
                    <fieldset class="flex gap-2">
                        <input type="radio" id="star1" name="rating" value="1" class="hidden" />
                        <label for="star1" class="cursor-pointer">
                            <i class="fas fa-star text-gray-400 hover:text-yellow-500"></i>
                        </label>
                        <input type="radio" id="star2" name="rating" value="2" class="hidden" />
                        <label for="star2" class="cursor-pointer">
                            <i class="fas fa-star text-gray-400 hover:text-yellow-500"></i>
                        </label>
                        <input type="radio" id="star3" name="rating" value="3" class="hidden" />
                        <label for="star3" class="cursor-pointer">
                            <i class="fas fa-star text-gray-400 hover:text-yellow-500"></i>
                        </label>
                        <input type="radio" id="star4" name="rating" value="4" class="hidden" />
                        <label for="star4" class="cursor-pointer">
                            <i class="fas fa-star text-gray-400 hover:text-yellow-500"></i>
                        </label>
                        <input type="radio" id="star5" name="rating" value="5" class="hidden" />
                        <label for="star5" class="cursor-pointer">
                            <i class="fas fa-star text-gray-400 hover:text-yellow-500"></i>
                        </label>
                    </fieldset>
                </div>
                <div class="mb-4">
                    <label class="block text-gray-700 text-sm font-medium mb-2" for="desc">Description</label>
                    <textarea
                        class="w-full px-3 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500"
                        name="comment" id="desc" rows="3"></textarea>
                </div>
                <button type="submit"
                    class="bg-green-500 text-white py-2 px-4 rounded-lg hover:bg-green-600">Submit</button>
            </form>
            <div class="mt-6">
                <% for(let review of product.reviews) { %>
                    <div class="bg-white shadow-md rounded-lg mb-4">
                        <div class="p-4">
                            <div class="flex flow-root">
                                <p class="text-blue-500 font-medium float-left"><%= currentUser.username %></p> 
                                <p class="text-gray-700 mb-2 flex float-right">
                                    
                                    Rated: <%= review.rating %> stars
                                </p>
                            </div>
                            <p class="text-gray-600"><%- review.comment %></p>
                            <button class="bg-red-500 text-white py-1 px-3 rounded-lg hover:bg-red-600 mt-2">Delete</button>
                        </div>
                    </div>
                <% } %>
            </div>
        </div>
    </div>
</section>

<!-- QR Payment Popup -->
<div id="popupForm" class="fixed inset-0 flex items-center justify-center bg-black bg-opacity-50 hidden">
    <div class="bg-white p-6 rounded-lg shadow-lg w-96">
        <div class="flex justify-between items-center mb-4">
            <h2 class="text-xl font-semibold">Complete Your Purchase</h2>
            <button onclick="hidePopup()" class="text-gray-500 hover:text-gray-700">
                <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12"/>
                </svg>
            </button>
        </div>
        <div class="text-center mb-4">
            <img src="/Frontend/Images/QR.jpg" alt="QR Code" class="w-52 h-52 mx-auto mb-4">
            <p class="text-gray-700">Scan the QR code to complete your payment</p>
        </div>
        <div class="mb-4">
            <label for="transactionId" class="block text-gray-700 text-sm font-medium mb-2">Transaction ID</label>
            <input type="text" id="transactionId"
                class="w-full px-3 py-2 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-blue-500">
        </div>
        <div class="mb-4 flex justify-around">
            <img src="data:image/png;base64," alt="Paytm" class="w-12 h-12">
            <img src="data:image/png;base64," alt="Google Pay" class="w-12 h-12">
            <img src="data:image/jpeg;base64," alt="PhonePe" class="w-12 h-12">
            <img src="data:image/png;base64," alt="UPI" class="w-12 h-12">
                    
            <!-- Add more icons as needed -->
        </div>
        <button id="finishButton"
            class="w-full bg-green-500 text-white py-2 px-4 rounded-lg hover:bg-green-600">Finish</button>
        <p id="errorMessage" class="text-red-500 mt-2 hidden">Transaction ID cannot be empty.</p>
    </div>
</div>

<!-- Thank You Popup -->
<div id="thankYouPopup" class="fixed inset-0 flex items-center justify-center bg-black bg-opacity-50 hidden">
    <div class="bg-white p-6 rounded-lg shadow-lg w-80 text-center">
        <h2 class="text-xl font-semibold mb-4">Thanks for Shopping!</h2>
        <p class="text-gray-700 mb-4">You will get a mail verification after evaluation of your payment.</p>
        <p id="countdown" class="text-gray-600 mb-4">Redirecting in <span id="timer">5</span> seconds...</p>
        <button class="bg-green-500 text-white py-2 px-4 rounded-lg hover:bg-green-600" onclick="window.location.href='/products';">Redirect Now</button>
    </div>
</div>

<script>
    function showPopup() {
        document.getElementById('popupForm').classList.remove('hidden');
    }

    function hidePopup() {
        document.getElementById('popupForm').classList.add('hidden');
    }

    // Handle finish button
    document.getElementById('finishButton').addEventListener('click', function() {
        const transactionId = document.getElementById('transactionId').value.trim();
        const errorMessage = document.getElementById('errorMessage');
        if (transactionId === '') {
            errorMessage.classList.remove('hidden');
        } else {
            errorMessage.classList.add('hidden');
            document.getElementById('popupForm').classList.add('hidden');
            document.getElementById('thankYouPopup').classList.remove('hidden');

            // Countdown timer for redirect
            let countdown = 5;
            const timerElement = document.getElementById('timer');
            const interval = setInterval(() => {
                countdown--;
                timerElement.textContent = countdown;
                if (countdown === 0) {
                    clearInterval(interval);
                    window.location.href = '/products';
                }
            }, 1000);
        }
    });

    // Handle wishlist toggle
    document.querySelectorAll('.wishlist-toggle').forEach(item => {
        item.addEventListener('click', event => {
            const productId = event.currentTarget.getAttribute('data-product-id');
            fetch(`/products/${productId}/toggle-wishlist`, {
                method: 'POST',
                headers: {
                    'Content-Type': 'application/json'
                },
                body: JSON.stringify({ productId })
            })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    event.currentTarget.querySelector('i').classList.toggle('far');
                    event.currentTarget.querySelector('i').classList.toggle('fas');
                } else {
                    console.error('Error toggling wishlist');
                }
            })
            .catch(error => console.error('Error:', error));
        });
    });


    // Countdown timer function
    function startCountdown() {
        let timer = 5;
        const timerElement = document.getElementById('timer');
        const countdownInterval = setInterval(() => {
            if (timer <= 0) {
                clearInterval(countdownInterval);
                window.location.href = '/products'; // Redirect when countdown ends
            } else {
                timerElement.textContent = timer;
                timer--;
            }
        }, 1000);
    }

    // Add event listeners to update star color based on selection
    document.querySelectorAll('input[name="rating"]').forEach((radio) => {
        radio.addEventListener('change', (event) => {
            const selectedRating = event.target.value;
            document.querySelectorAll('label i').forEach((star, index) => {
                if (index < selectedRating) {
                    star.classList.add('text-yellow-500');
                    star.classList.remove('text-gray-400');
                } else {
                    star.classList.add('text-gray-400');
                    star.classList.remove('text-yellow-500');
                }
            });
        });
    });
</script>

<!-- Include FontAwesome for star icons -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
