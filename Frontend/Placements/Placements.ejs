  <!-- Main Content -->
  <main class="main-content -mt-16">
    <!-- Banner -->
    <div class="banner darkable">
      <p>Contact Somnath Kaushik for discount coupon Codes on Coding Blocks Courses <a
          href="mailto:somnath.kaushik_cs21@gla.ac.in">
          <d>somnath.kaushik_cs21@gla.ac.in</d>
        </a></p>
      <button id="closeBanner">X</button>
    </div>
  

  <script>
    // Function to handle side nav state on window resize
    function handleSideNav() {
      var sideNav = document.getElementById('side-nav');
      var mainContent = document.querySelector('.main-content');
      if (window.innerWidth <= 768) {
        sideNav.classList.add('shrink');
        mainContent.classList.add('shrink');
      } else {
        sideNav.classList.remove('shrink');
        mainContent.classList.remove('shrink');
      }
    }

    // Toggle side nav visibility with button
    document.getElementById('side-nav-toggle').addEventListener('click', function () {
      var sideNav = document.getElementById('side-nav');
     // var mainContent = document.querySelector('.main-content');
      sideNav.classList.toggle('shrink');
      mainContent.classList.toggle('shrink');
    });

    // JavaScript to handle banner removal
    document.getElementById('closeBanner').addEventListener('click', function () {
      var banner = document.querySelector('.banner');
      banner.style.display = 'none';
    });

    // Initial call to set the state on page load
    handleSideNav();

    // Adjust side nav on window resize
    window.addEventListener('resize', handleSideNav);
  </script>


  <!-- Content Area -->
  <div id="content-area" class="">
    <%- include('./tech_blogs') %>
  </div>

  <div id="content-area1" class="" style="display: none;">
    <%- include('./internships.ejs') %>
  </div>

  <div id="content-area2" class="" style="display: none;">
    <%- include('./jobs_updates.ejs') %>
  </div>

  <div id="content-area3" class="" style="display: none;">
    <%- include('./build_resume.ejs') %>
  </div>

  <div id="content-area4" class="" style="display: none;">
    <%- include('./notes.ejs') %>
  </div>

  <div id="content-area5" class="" style="display: none;">
    <%- include('./gifts.ejs') %>
  </div>

  <div id="content-area6" class="" style="display: none;">
    <%- include('./tips_for_jobs.ejs') %>
  </div>

  <script>
    document.addEventListener('DOMContentLoaded', () => {
      const navItems = document.querySelectorAll('.nav-item');
      const contentArea = document.getElementById('content-area');
      const contentArea1 = document.getElementById('content-area1');
      const contentArea2 = document.getElementById('content-area2');
      const contentArea3 = document.getElementById('content-area3');
      const contentArea4 = document.getElementById('content-area4');
      const contentArea5 = document.getElementById('content-area5');
      const contentArea6 = document.getElementById('content-area6');

      // Function to load content based on the selected option

      function loadContent(contentId) {

        navItems.forEach(item => {
          item.classList.remove('skyblue');
        });

        if (contentId === 'tech-blogs') {
          contentArea.style.display = 'block';
          contentArea1.style.display = 'none';
          contentArea2.style.display = 'none';
          contentArea3.style.display = 'none';
          contentArea4.style.display = 'none';
          contentArea5.style.display = 'none';
          contentArea6.style.display = 'none';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        } else if (contentId === 'internships') {
          contentArea.style.display = 'none';
          contentArea1.style.display = 'block';
          contentArea2.style.display = 'none';
          contentArea3.style.display = 'none';
          contentArea4.style.display = 'none';
          contentArea5.style.display = 'none';
          contentArea6.style.display = 'none';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        }
        else if (contentId === 'jobs-updates') {
          contentArea.style.display = 'none';
          contentArea1.style.display = 'none';
          contentArea2.style.display = 'block';
          contentArea3.style.display = 'none';
          contentArea4.style.display = 'none';
          contentArea5.style.display = 'none';
          contentArea6.style.display = 'none';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        }
        else if (contentId === 'build-resume') {
          contentArea.style.display = 'none';
          contentArea1.style.display = 'none';
          contentArea2.style.display = 'none';
          contentArea3.style.display = 'block';
          contentArea4.style.display = 'none';
          contentArea5.style.display = 'none';
          contentArea6.style.display = 'none';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        }
        else if (contentId === 'notes') {
          contentArea.style.display = 'none';
          contentArea1.style.display = 'none';
          contentArea2.style.display = 'none';
          contentArea3.style.display = 'none';
          contentArea4.style.display = 'block';
          contentArea5.style.display = 'none';
          contentArea6.style.display = 'none';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        }
        else if (contentId === 'gifts') {
          contentArea.style.display = 'none';
          contentArea1.style.display = 'none';
          contentArea2.style.display = 'none';
          contentArea3.style.display = 'none';
          contentArea4.style.display = 'none';
          contentArea5.style.display = 'block';
          contentArea6.style.display = 'none';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        }
        else if (contentId === 'tips-for-jobs') {
          contentArea.style.display = 'none';
          contentArea1.style.display = 'none';
          contentArea2.style.display = 'none';
          contentArea3.style.display = 'none';
          contentArea4.style.display = 'none';
          contentArea5.style.display = 'none';
          contentArea6.style.display = 'block';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        } else {
          // Handle other content sections or make an AJAX request
          contentArea.innerHTML = `<h2>${contentId.replace('-', ' ').toUpperCase()} Content</h2>`;
          contentArea.style.display = 'block';
          contentArea1.style.display = 'none';
          contentArea2.style.display = 'none';
          contentArea3.style.display = 'none';
          contentArea4.style.display = 'none';
          contentArea5.style.display = 'none';
          contentArea6.style.display = 'none';
          document.querySelector(`[data-content='${contentId}']`).classList.add('skyblue');
        }
      }

      // Load Tech Blogs content by default
      loadContent('tech-blogs');

      navItems.forEach(item => {
        item.addEventListener('click', () => {
          const contentId = item.getAttribute('data-content');
          loadContent(contentId);
        });
      });
    });
  </script>


</main>












