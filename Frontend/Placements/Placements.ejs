  <title>Learning Heaven</title>
  <style>
    body::-webkit-scrollbar {
      display: none;
    }
    

    /* Animation for the bars */
  .bar {
    transition: transform 0.3s ease, opacity 0.3s ease;
  }

  /* Styles applied when the button is toggled */
  #side-nav-toggle.active .bar:nth-child(1) {
    transform: rotate(45deg) translate(4px, 4px);
  }

  #side-nav-toggle.active .bar:nth-child(2) {
    opacity: 0;
  }

  #side-nav-toggle.active .bar:nth-child(3) {
    transform: rotate(-45deg) translate(4px, -4px);
  }

    /* Style for the fixed side nav */
    .side-nav {
      position: absolute;
      top: 6rem;
      left: 0;
      width: 16rem;
      /* Default width for larger screens */
      height: auto;
      background-color: #ffffff;
      color: rgb(0, 0, 0);
      padding: 1rem;
      box-shadow: 2px 0 4px rgba(0, 0, 0, 0.1);
      transition: width 0.3s ease;
    }

    .side-nav li:hover {
      background-color: rgb(0, 183, 255);
    }

    .side-nav.shrink {
      width: 5rem;
      /* Width when shrunk */
    }

    .main-content {
      margin-left: 16rem;
      padding-top: 4rem;
      transition: margin-left 0.3s ease;
    }

    .main-content.shrink {
      margin-left: 4rem;
    }

    .side-nav.shrink ul li a {
      text-align: center;
    }

    .side-nav.shrink ul li a span {
      display: none;
    }

    .side-nav .nav-item {
      display: flex;
      align-items: center;
      margin-bottom: 1rem;
      transition: opacity 0.3s ease;
    }

    .side-nav .nav-item img {
      width: 1.5rem;
      height: 1.5rem;
      margin-right: 1rem;
    }

    .side-nav .nav-item span {
      display: inline-block;
      transition: opacity 0.3s ease;
    }

    .side-nav.shrink .nav-item span {
      opacity: 0;
    }

    @media (max-width: 768px) {
      .side-nav {
        width: 16rem;
      }

      .side-nav.expanded {
        width: 16rem !important;
        /* Expanded width for small screens when toggled */
      }

      .main-content {
        margin-left: 4rem;
        /* Margin for small screens */
      }

      .main-content.shrink {
        margin-left: 4rem;
        /* Margin when side nav is shrunk */
      }


    }
    footer{
      margin-top: 32rem;
      z-index: 1000 !important;
    }
  </style>
</head>

<body>

  <!-- Navbar -->
  <nav class="skyblue p-4 darkable" id="navlogo">
    <div class="container mx-auto flex justify-between items-center ">
