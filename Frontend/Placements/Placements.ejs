          xmlns="http://www.w3.org/2000/svg">
          <path id="menu-icon-line1" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16"></path>
          <path id="menu-icon-line2" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 12h16">
          </path>
          <path id="menu-icon-line3" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 18h16">
          </path>
        </svg>
      </button>

      <!-- Navbar Links -->
      <ul class="hidden lg:flex space-x-4">
        <li><a href="/" class="darkable  nav-link text-white ">Home</a></li>
        <li><a href="/courses" class="darkable  nav-link text-white ">Courses</a></li>
        <li><a href="/resources" class="darkable  nav-link text-white ">Resources</a></li>
        <li><a href="/contact" class="darkable  nav-link text-white ">Contact</a></li>
        <li><a href="/products" class="darkable  nav-link text-white ">Products</a></li>
        <li><a href="/Placements" class="darkable  nav-link text-white ">Placements</a></li>
        <li><a href="/Compiler" class="darkable  nav-link text-white ">Compiler</a></li>
      </ul>
    </div>
  </nav>

  <!-- Responsive Menu -->
  <div class="lg:hidden hidden mt-10 " id="menu">
    <ul class="bg-gray-800 p-2 text-center">
      <li><a href="/" class="darkable block text-white py-2">Home</a></li>
      <li><a href="/courses" class="darkable block text-white py-2">Courses</a></li>
      <li><a href="/resources" class="darkable block text-white py-2">Resources</a></li>
      <li><a href="/contact" class="darkable block text-white py-2">Contact</a></li>
      <li><a href="/products" class="darkable block text-white py-2">Products</a></li>
      <li><a href="/placements" class="darkable block text-white py-2">Placements</a></li>
      <li><a href="/Compiler" class="darkable block text-white py-2">Compiler</a></li>
    </ul>
