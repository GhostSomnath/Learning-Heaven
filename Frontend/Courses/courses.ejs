            document.addEventListener("DOMContentLoaded", function () {
                const descriptionLink1 = document.getElementById("descriptionLink1");
                const reviewsLink1 = document.getElementById("reviewsLink1");
                const detailsLink1 = document.getElementById("detailsLink1");
                const activeLine1 = document.getElementById("activeLine1");

                const descriptionContent1 = document.getElementById("descriptionContent1");
                const reviewsContent1 = document.getElementById("reviewsContent1");
                const detailsContent1 = document.getElementById("detailsContent1");

                // Show description by default
                descriptionContent1.style.display = "block";
                reviewsContent1.style.display = "none";
                detailsContent1.style.display = "none";

                // Set initial position of the active line
                activeLine1.style.transform = "translateX(0)";

                descriptionLink1.addEventListener("click", function () {
                    if (!descriptionLink1.classList.contains("active-link")) {
                        descriptionContent1.style.display = "block";
                        reviewsContent1.style.display = "none";
                        detailsContent1.style.display = "none";

                        // Move active line to description
                        activeLine1.style.transform = "translateX(0)";

                        // Update link colors
                        descriptionLink1.classList.add("active-link");
                        reviewsLink1.classList.remove("active-link");
                        detailsLink1.classList.remove("active-link");

                        // Make description link color black
                        descriptionLink1.classList.remove("inactive-link");
                        reviewsLink1.classList.add("inactive-link");
                        detailsLink1.classList.add("inactive-link");
                    }
                });

                reviewsLink1.addEventListener("click", function () {
                    if (!reviewsLink1.classList.contains("active-link")) {
                        descriptionContent1.style.display = "none";
                        reviewsContent1.style.display = "block";
                        detailsContent1.style.display = "none";

                        // Move active line to reviews
                        activeLine1.style.transform = "translateX(100%)";

                        // Update link colors
                        descriptionLink1.classList.remove("active-link");
                        reviewsLink1.classList.add("active-link");
                        detailsLink1.classList.remove("active-link");

                        // Make reviews link color black
                        descriptionLink1.classList.add("inactive-link");
                        reviewsLink1.classList.remove("inactive-link");
                        detailsLink1.classList.add("inactive-link");
                    }
                });

                detailsLink1.addEventListener("click", function () {
                    if (!detailsLink1.classList.contains("active-link")) {
                        descriptionContent1.style.display = "none";
                        reviewsContent1.style.display = "none";
                        detailsContent1.style.display = "block";

                        // Move active line to details
                        activeLine1.style.transform = "translateX(200%)";

                        // Update link colors
                        descriptionLink1.classList.remove("active-link");
                        reviewsLink1.classList.remove("active-link");
                        detailsLink1.classList.add("active-link");

                        // Make details link color black
                        descriptionLink1.classList.add("inactive-link");
                        reviewsLink1.classList.add("inactive-link");
                        detailsLink1.classList.remove("inactive-link");
                    }
                });
            });
        </script>



